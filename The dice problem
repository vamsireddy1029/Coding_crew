//{ Driver Code Starts
#include<bits/stdc++.h> 
using namespace std; 

// } Driver Code Ends
//User function Template for C++
class Solution
{
public:
    int oppositeFaceOfDice(int N)
    {
        // Write Your Code here
        int res  = 0;
        if(N == 6){
            res = 1;
        }
        else if(N == 5){
            res = 2;
        }
        else if(N == 4){
            res = 3;
        }
        else if(N == 3){
            res = 4;
        }
        else if(N == 2){
            res = 5;
        }
        else{
            res = 6;
        }
        return res;
    }
};

//{ Driver Code Starts.
int main()
{
    int t;
    cin >> t;
    while (t--)
    {
        int N;
        cin>>N;
        Solution ob;
        int ans  = ob.oppositeFaceOfDice(N);
        cout<<ans<<endl;
    }
    return 0;
}
// } Driver Code Ends
